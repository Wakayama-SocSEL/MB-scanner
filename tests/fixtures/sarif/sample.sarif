{
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeQL",
          "version": "2.23.1"
        }
      },
      "results": [
        {
          "ruleId": "js/test/example",
          "message": {
            "text": "This is a test detection."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/example.js",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 5,
                  "startColumn": 10,
                  "endLine": 7,
                  "endColumn": 2
                }
              }
            }
          ],
          "level": "warning"
        },
        {
          "ruleId": "js/test/example",
          "message": {
            "text": "Another test detection without columns."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/example.js",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 10,
                  "endLine": 12
                }
              }
            }
          ],
          "level": "error"
        },
        {
          "ruleId": "js/test/example",
          "message": {
            "text": "Detection in another file."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/utils.js",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 1,
                  "endLine": 3
                }
              }
            }
          ],
          "level": "note"
        },
        {
          "ruleId": "js/test/example",
          "message": {
            "text": "Detection without endLine (single line)."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/example.js",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 2,
                  "endColumn": 20
                }
              }
            }
          ],
          "level": "warning"
        }
      ]
    }
  ]
}
